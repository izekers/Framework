<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zoker.framework">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name="com.zoker.common.BaseApplication"
        android:theme="@style/AppTheme">
        <activity android:name=".view.activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".view.activity.MainActivity"/>
        <activity android:name=".view.activity.Mul_GlobalActivity"/>
        <activity android:name=".view.activity.Mul_SimpleActivity"/>
        <activity android:name=".view.activity.MultiTypeActivity"/>
    </application>

</manifest>
